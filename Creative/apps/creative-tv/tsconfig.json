{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "target": "ES2017",
    "baseUrl": "./src",
    "forceConsistentCasingInFileNames": true
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
